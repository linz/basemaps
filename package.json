    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "lerna": "^3.18.2",
    "ospec": "^4.0.1",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
